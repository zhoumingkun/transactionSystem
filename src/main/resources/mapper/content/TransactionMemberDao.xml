<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.toughguy.transactionSystem.model.content.po.TransactionMember">

	<!-- 通过openid查询该用户是否绑定过会员卡 -->
	<select id="findmeber" parameterType="TransactionMember" resultType="TransactionMember">
		select * from transaction_member where member_tel=#{memberTel} and member_pwd=#{memberPwd}
	</select>
	
	<!-- 通过openid查询该用户是否绑定过会员卡 -->
	<select id="findMemberAll"  resultType="TransactionMember">
		select * from transaction_member
	</select>
	
</mapper>