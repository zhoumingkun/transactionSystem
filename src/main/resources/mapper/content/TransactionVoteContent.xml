<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.toughguy.transactionSystem.model.content.po.TransactionVoteContent">
	<select id="findAll" resultType="TransactionVoteContent">
		select * from transaction_vote_content;
	</select>
	
	<update id="update" parameterType="TransactionVoteContent">
		update transaction_vote_content
		set vote_content=#{voteContent}
		where vote_content_id=#{voteContentId};
	</update>
	
	<delete id="delete" parameterType="TransactionVoteContent">
		delete from transaction_vote_content
		where vote_content_id=#{voteContentId};
	</delete>
	
	<insert id="save" parameterType="TransactionVoteContent">
		insert into transaction_vote_content(vote_content)
		values(#{voteContent});	
	</insert>
	
	<insert id="insertVoteContent" parameterType="TransactionVoteContent" useGeneratedKeys="true" keyProperty="voteContentId">
		insert into transaction_vote_content(vote_content)
		values(#{voteContent});	
	</insert>
</mapper>